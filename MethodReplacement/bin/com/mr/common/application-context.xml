<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
	<bean id="insuranceService" class="com.mr.bean.InsuranceService">
		<replaced-method name="estimatePremiumAmount"
			replacer="estimatePremiumAmountReplacer">
			<arg-type>java.lang.String</arg-type>
			<arg-type>long</arg-type>
			<arg-type>int</arg-type>
			<arg-type>int</arg-type>
			<arg-type>java.lang.String</arg-type>
			
		</replaced-method>
	</bean>

	<bean id="estimatePremiumAmountReplacer"
		class="com.mr.bean.EstimatePremiumAmountReplacer" />

</beans>
