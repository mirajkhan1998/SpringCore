<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<bean id="car" class="com.qualifier.annotation.Car" />
	<bean id="engine1" class="com.qualifier.annotation.Engine">
		<property name="mode" value="1" />
		<property name="type" value="BS6" />
	</bean>
	<bean id="engine2" class="com.qualifier.annotation.Engine">
	   <property name="mode" value="2" />
	   <property name="type" value="BS4" />
	   <qualifier value="bs4Engine" />
	</bean>

	<context:annotation-config />

</beans>
