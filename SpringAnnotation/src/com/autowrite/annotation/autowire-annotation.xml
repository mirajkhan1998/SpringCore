<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<bean id="radio" class="com.autowrite.annotation.Radio">
	</bean>
    <bean id="reciever1" class="com.autowrite.annotation.Reciever">
		<property name="frequency" value="200" />
		<property name="band" value="AM" />
	</bean>
	<!--  <bean id="reciever2" class="com.autowrite.annotation.Reciever">
	  <property name="frequency" value="100" />
	  <property name="band" value="PM"/>
	</bean>-->
	<bean id="controller" class="com.autowrite.annotation.NoiseController" />
	<context:annotation-config />

</beans>
